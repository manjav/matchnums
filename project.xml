<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="Match Nums" package="com.grantech.matchnums" version="0.0.1" company="GranTech" />
	<app path="bin" file="MatchNums" main="com.grantech.matchnums.Main" />
	
	<window width="480" height="920" if="desktop" />
	<window width="0" height="0" if="html5" />
	
	<source path="src" />
	
	<haxelib name="openfl" />
	<haxelib name="actuate" />

	<icon path="Assets/images/icon.svg" />
	<assets path="Assets/images" rename="images" include="*" exclude="icon.svg" />
	<assets path="Assets/fonts" rename="fonts" include="*.ttf" />
	
	<!-- <config:android target-sdk-version="28"/> -->
	<section if="release-sign">
		<certificate path="certs/bbg.keystore" password="123456" alias="boom" alias-password="654321" if="android" />
		<certificate path="certs/bbg.p12" if="blackberry" />
	</section>

</project>
